{
  "name": "matchless-website",
  "version": "1.0.0",
  "description": "An ecommerce clothing website",
  "main": "server.js",
  "scripts": {
    "angular": "cd client && ng serve --open ",
    "start": "cross-env NODE_ENV=production node server",
    "start_cross": "cross-env NODE_ENV=production node server",
    "start_dev": "node server",
    "dev": "concurrently \"npm run start_dev\" \"npm run angular\"",
    "install-client": "cd client && npm i ",
    "install-server": "npm i",
    "install-dep": "concurrently \"npm run install-server\" \"npm run install-client\""
  },
  "author": "The Ball Fondlers",
  "license": "MIT",
  "dependencies": {
    "angular-bootstrap-md": "^8.8.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.0.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.1.4",
    "firebase": "^7.6.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.4",
    "morgan": "^1.9.1",
    "react-bootstrap": "^1.0.0-beta.16",
    "stripe": "^8.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
